{
    "name": "mosweed/laravel-crud-generator",
    "description": "Automatische CRUD generator voor Laravel 12 - genereert migrations, seeders, factories, models, controllers, views, routes en relaties",
    "keywords": ["laravel", "crud", "generator", "scaffolding", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mosweed",
            "email": "info@mosweed.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/filesystem": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Mosweed\\CrudGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mosweed\\CrudGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mosweed\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
